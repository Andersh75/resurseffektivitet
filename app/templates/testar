            $(function(){
              $("a").each(function(i){
                len=$(this).text().length;
                if(len>66)
                {
                  $(this).text($(this).text().substr(0,66)+'...');
                }
              });
              $("#daterader p").each(function(i){
                len=$(this).text().length;
                if(len>6)
                {
                  $(this).text($(this).text().substr(2,7));
                }
              });
            });


            var removeIntent = false;

            $('[id^=dest]').sortable({
                update: function() {
                    var orders = [];
                    $.each($(this).children(), function(i, item) {
                        orders.push($(item).data("x"));
                    });

                    $("#info").text("Order: " + orders.join(","));
                },
                over: function () {
                removeIntent = false;
                },
                out: function () {
                    removeIntent = true;
                },
                beforeStop: function (event, ui) {
                    if(removeIntent == true){
                        ui.item.remove();
                    }
                }

            });




                        $('a#process_input').bind('click', function() {
                            $.getJSON('/background_process', {
                              proglang: $('input[name="proglang"]').val(),
                            }, function(result) {
                                //var varresult = JSON.stringify(result);
                                var resultet = result
                                alert(resultet);
                            });
                            return false;
                        });
                    });

                        $('[id^=selectteacher]').editable({
                            mode: 'popup',
                            type: 'select',
                            title: 'Select status',
                            placement: 'right',
                            value: 2,
                            source: resultet
                            /*
                             //uncomment these lines to send data on server
                             ,pk: 1
                             ,url: '/post'
                             */

                             type: 'select',
                             mode: popup,
                             placement: top,
                             autotext : 'never',
                             source : getSource()

                        function getSource() {
                            var url = "/background_process";
                            return $.ajax({
                                type:  'GET',
                                async: true,
                                url:   url,
                                dataType: "json"
                            });
                        };

                        getSource().done(function(result) {

                            $('[id^=selectteacher]').editable({
                                type: 'select',
                                title: 'Select status',
                                placement: 'right',
                                value: 2,
                                source: [
                                    {value: 1, text: 'first'},
                                    {value: 2, text: 'second'},
                                    {value: 3, text: 'third'}
                                ]
                                /*
                                 //uncomment these lines to send data on server
                                 ,pk: 1
                                 ,url: '/post'
                                 */
                            });
                        });




