        <!-- Modal -->
        <div class="modal fade" id="edit-slot-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">

          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Edit class</h4>
              </div>

              <form class="form-horizontal" id="myForm" role="form">

                <input type="text" id="entityKey" name="entity_key" class="hidden">
                <div class="modal-body">
                  <div class="form-group">
                    <label class="col-sm-2 control-label">Content</label>
                    <div class="col-sm-10">
                      <input type="text" name="content" id="content" class="form-control show">
                    </div>
                  </div>


                    <div class="form-group">
                      <label class="col-sm-2 ">About the class</label>
                      <div class="col-sm-10">
                          <textarea class="form-control" name="info" id="info" rows="1"></textarea>
                      </div>
                    </div>


                    <!--
                    <div class="form-group">
                      <label class="col-sm-2 ">Teachers</label>
                      <div class="col-sm-10">
                          <select class="js-example-tags-teachers form-control select2-hidden-accessible" id="tags" multiple="multiple" name="tags" tabindex="-1" aria-hidden="true"></select>
                      </div>
                    </div>
                -->




                </div>




                <div class="modal-footer">
                <button type="button" id="errorAlert" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="submit" onclick="saveData()" id="successAlert" class="btn btn-primary">Save</button>
                </div>
              </form>
            </div>
          </div>
        </div>
    </div> <!-- /container -->



		{% if session.logged_in %}
			<p>
				{% block rooms %}
