<script src="{{ url_for('static', filename='js/jquery.select2.js') }}"></script>-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/bootstrap3-editable/js/bootstrap-editable.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.2.0/js/tether.min.js" integrity="sha384-Plbmg8JY28KFelvJVai01l8WyZzrYWG825m+cZ0eDDS1f7d/js6ikvy1+X+guPIB" crossorigin="anonymous"></script>




<style type = "text/css">
.form-horizontal .control-label{
  /* text-align:right; */
  text-align:left;
}

.some_class{
   width: 300px !important;
}
</style>


</head>


<body>






	<div class="container">
		<nav class="navbar navbar-default">
		  <div class="container-fluid">
		    <!-- Brand and toggle get grouped for better mobile display -->
			<ul class="nav navbar-nav">
		        <li><a href="{{ url_for('login_page') }}">Starter1</a></li>
				{% if session.logged_in %}
		        <li><a href="{{ url_for('rooms_page', page=6)}}">Rooms</a></li>
                <li><a href="{{ url_for('teachers_page', page=6)}}">Teachers</a></li>
                <li><a href="{{ url_for('courses_page')}}">Courses</a></li>
                <li><a href="{{ url_for('myinfo_page')}}">My info</a></li>
				{% endif %}
			</ul>



		    <!-- Collect the nav links, forms, and other content for toggling -->
		    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
			  <div class="nav navbar-nav navbar-right">
			  	{% if session.logged_in %}
					<a class="btn btn-default navbar-btn" href="{{ url_for('logout') }}">Sign out</a>
					<!--<button type="submit" class="btn btn-default navbar-btn">Sign out</button>-->
			  	{% else %}
					{%- if request.path != "/register/" %}
						<a class="btn btn-default navbar-btn" href="{{ url_for('register_page') }}">Register</a>
						{% endif %}
					{% block login %}{% endblock %}

					<!--<a class="btn btn-default navbar-btn" href="{{ url_for('login_page') }}">Sign in</a>
					<button type="submit" class="btn btn-default navbar-btn">Sign in</button>-->
			    {% endif %}
		  	  </div>
		    </div><!-- /.navbar-collapse -->
		  </div><!-- /.container-fluid -->
		</nav>





    </div> <!-- /container -->


	<div class ="container">


		<div class="jumbotron">


			{% if session.logged_in %}
				<p>
					{% block rooms %}
					{% endblock %}
                    {% block teachers %}
					{% endblock %}
                    {% block courses %}
                    TEXT FÖR INLOGGAD
                    {% set me = myobject() %}
                    {{ me.password }}
                    {% endblock %}
				</p>
			{% else %}
				{% block register %}
					TEXT FÖR UTLOGGAD OM EJ REGISTRERING
				{% endblock %}
			{% endif %}
</div>

{% with messages = get_flashed_messages() %}
    {% if messages %}
        {% for message in messages %}
            <div class="alert alert-warning alert-dismissible" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                {{message}}
            </div>
        {% endfor %}
    {% endif %}
{% endwith %}
</div>

