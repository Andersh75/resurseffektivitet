    <script type="text/javascript">
        $(document).ready(function() {

            //$.fn.editable.defaults.mode = 'inline';


            $('#fel').editable({
                title: 'Select C',
                inputclass: 'some_class'
            });



            $(function  () {
                $("tbody").sortable();
            });



            $('[id^=coursename]').editable({
                title: 'Select Course',
                inputclass: 'some_class'
            });


            $('[id^=courseexaminer]').editable({
                title: 'Select Course',
                inputclass: 'some_class'
            });




        });
    </script>


  </body>


</html>
