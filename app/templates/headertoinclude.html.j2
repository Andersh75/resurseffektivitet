                {% block teachers %}
				{% endblock %}
                {% block onecourse %}
                {% endblock %}
                {% block oneroom %}
                {% endblock %}
                {% block oneteacher %}
                {% endblock %}
                {% block oneslot %}
                {% endblock %}
                {% block allrooms %}
                {% endblock %}
                {% block allcourses %}
                {% endblock %}
                {% block allteachers %}
                {% endblock %}
                {% block courses %}

                {% set me = myobject() %}

                {% endblock %}

			</p>
		{% else %}
			{% block register %}
            <div class="container">
                <div class="jumbotron">
                TEXT FÖR UTLOGGAD OM EJ REGISTRERING
                </div>
            </div>

			{% endblock %}

		{% endif %}

<div class="container">

    {% with messages = get_flashed_messages() %}
        {% if messages %}
            {% for message in messages %}
                <div class="alert alert-warning alert-dismissible" role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                    {{message}}
                </div>
            {% endfor %}
        {% endif %}
    {% endwith %}

</div>



